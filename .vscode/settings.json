{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules/": true,
    "*.lock": true,
    "*.log": true
  },
  "cSpell.words": [
    "codegen",
    "parens"
  ]
}
